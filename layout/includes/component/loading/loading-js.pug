script.
    var preloader = {
        endLoading: () => {
            document.body.style.overflow = 'auto';
            document.getElementById('loading-box').classList.add("pre-loaded")
        },
        initLoading: () => {
            document.body.style.overflow = 'hidden';
            document.getElementById('loading-box').classList.remove("pre-loaded")

        }
    }
    window.addEventListener('load',preloader.endLoading())